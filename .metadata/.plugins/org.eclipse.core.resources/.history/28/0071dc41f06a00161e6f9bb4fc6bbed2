package adf;

import java.awt.Window.Type;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.LinkedList;
import java.util.Map;

interface IAdd<T>{
	public void add(T name, T str1, T str2, T str3); 
}

interface IInspect<T> {
	public void inspect(T type);
}

interface ISet<T> {
	public void set(T type);
}

interface IModif<T> {
	public void modif(int index);
}

class MyContainer<T> {
	T name;
	
	ArrayList<String> arr = new ArrayList<>();

	public void add(T name, T str1, T str2, T str3) {
		arr.add((String) name);
		arr.add((String) str2);
		arr.add((String) str3);
		arr.add((String) str1);
		System.out.println(arr);
	}

}

public class Student<T> implements IInspect<T>, ISet<T>, IModif<T>{


	@Override
	public void modif(int index) {
	}

	@Override
	public void set(T type) {
	}

	@Override
	public void inspect(T type) {
	}



	public static <T> void main(String[] args) {
		MyContainer<String> mc = new MyContainer();

		mc.add("Petrov", "modif", "set", "inspect");

	}

}
