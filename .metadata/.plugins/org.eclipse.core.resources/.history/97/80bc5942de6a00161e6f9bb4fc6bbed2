package adf;

import java.awt.Window.Type;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.LinkedList;
import java.util.Map;

interface IInspect<T>{
	public void inspect(T type);
}

interface ISet<T>{
	public void set(T type);
}

interface IModif<T>{
	public void modif(int index);
}

class MyContainer<T>{
	T name;
	public void add(String str, String str2, String str3, String str4){};
	public void inspectAdd(T type) {}
	public void setAdd(T type) {}
	public void modifAdd(int index) {}
}

class List<T> implements IInspect<T>, ISet<T>, IModif<T> {
	ArrayList<String> arr = new ArrayList<>();
	
	public void add(String str, String str2, String str3, String str4){
		arr.add(str);
		arr.add(str2);
		arr.add(str3);
		arr.add(str4);
		System.out.println(arr);
	}
	
	@Override
	public void modif(int index) {
	}

	@Override
	public void set(T type) {
	}

	@Override
	public void inspect(T type) {
	}
	
}


public class Student<T> {
	
	
	
	public static <T> void main(String[] args) {
		MyContainer<String> mc = new MyContainer();
		
		mc.add("Petrov", "modif", "set", "inspect");
		
		
	}

}
