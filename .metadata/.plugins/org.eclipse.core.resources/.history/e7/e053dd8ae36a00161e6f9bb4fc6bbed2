package adf;

import java.awt.Window.Type;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.LinkedList;
import java.util.Map;

interface IInspect<T> {
	public void inspect(T type);
}

interface ISet<T> {
	public void set(T type);
}

interface IModif<T> {
	public void modif(int index);
}

class MyContainer<T> {
	T name;
	T str1;
	T str2;
	T str3;

	public MyContainer(T name, T str1, T str2, T str3) {
		this.name = name;
		this.str1 = str1;
		this.str2 = str2;
		this.str3 = str3;
	}
}

public class Student<T> implements IInspect<T>, ISet<T>, IModif<T> {

	ArrayList<MyContainer> arr = new ArrayList<>();

	@Override
	public void add(String name, String str1, String str2, String str3) {
		MyContainer mc1 = MyContainer(name, str1, str2, str3);
		arr.add(str);
		arr.add(str2);
		arr.add(str3);
		arr.add(str4);
		System.out.println(arr);
	}

	@Override
	public void modif(int index) {
	}

	@Override
	public void set(T type) {
	}

	@Override
	public void inspect(T type) {
	}

}

	public static <T> void main(String[] args) {
		MyContainer<String> mc = new MyContainer();

		mc.add("Petrov", "modif", "set", "inspect");

	}

}
