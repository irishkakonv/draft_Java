package NIIT;

class Outher{
	private int outerField;
	public void printOuter(){
		Inner i = new Inner();
		i.printInner();
		i.innerField = 10;
		System.out.println("Outer class");
	}
	public void printOuter2() {
		System.out.println("Outer class from Inner Class");
	}
	
	
	
	public static class Inner{
		private int innerField = 0;
		public void printInner(){
			System.out.println("Inner class");
			System.out.println();
		}
	}
}


public class InnerOuter {
	public static void main(String[] args) {
		Outher outer = new Outher();
		outer.printOuter();
		Outher.Inner outerInner = new Outher.Inner();  //создание объектв внутреннего static класса 
		outerInner.printInner();
		
		Outher.Inner oi = outer.new Inner();  //создание объекта внутреннего не statis класса
	}

}
