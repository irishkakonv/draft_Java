package NIIT;

class Aa extends Thread{
	@Override
	public void run() {
		Main26_2.prSeq(this.toString(), 10);
	}
}


public class Main26_2 {

	public static void main(String[] args)  {
		prSeq("main", 10);
		Aa a = new Aa();
		a.start();
		
	}
	
	static void prSeq(String label, int limit) {
		for(int i = 0; i < limit; i++){
			System.out.println(label + ":" + i);
			
		}
		
	}

}
