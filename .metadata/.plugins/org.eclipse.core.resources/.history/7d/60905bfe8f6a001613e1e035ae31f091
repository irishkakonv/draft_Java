package NIIT;

class Stack{
	private int arr[] = new int[10];
	private int top = 0;
	
	public void push(int value){
		arr[top++] = value;
		
	}
	public int pop(){
		return arr[top--];
	}
}

public class Main2 {

	public static void main(String[] args) {
		
		Stack st = new Stack();
		st.push(1);
		st.push(2);
		st.push(3);
		System.out.println(st.pop());
		System.out.println(st.pop());
		System.out.println(st.pop());
	}

}
