class MyContainer<T> {
	T name;
	
	public void add(T name, T str1, T str2, T str3) {}
	public void set(T type) {}
	public void modif(int index) {}
	public void inspect(T type) {}
}

public class Student<T> extends MyContainer<T>{
	ArrayList<String> arr = new ArrayList<>();
	
	@Override
	public void add(T name, T str1, T str2, T str3) {
		arr.add((String) name);
		arr.add((String) str2);
		arr.add((String) str3);
		arr.add((String) str1);
		System.out.println(arr);
	}
	
	@Override
	public void modif(int index) {
	}

	@Override
	public void set(T type) {
	}

	@Override
	public void inspect(T type) {
	}



	public static <T> void main(String[] args) {
		MyContainer<String> mc = new Student<>();

		mc.add("Petrov", "modif", "set", "inspect");
		

	}

}
