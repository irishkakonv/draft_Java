package NIIT;

class Outher{
	private static int outerField;
	public void printOuter(){
		Inner i = new Inner();
		i.printInner();
		i.innerField = 10;
		System.out.println("Outer class");
	}
	public static void printOuter2() {
		System.out.println("Outer class from Inner Class");
	}
	
	
	
	public static class Inner{
		private int innerField = 0;
		public void printInner(){
			System.out.println("Inner class");
			Outher.printOuter2();
			System.out.println(outerField);
		}
	}
}


public class InnerOuter {
	public static void main(String[] args) {
		Outher outer = new Outher();
		outer.printOuter();
		
		Outher.Inner outerInner = new Outher.Inner();
		outerInner.printInner();
		
		
	}

}
